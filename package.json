{
	"name": "astro-lintlapro",
	"type": "module",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"astro": "astro",
		"build": "astro check && tsc --noEmit && astro build",
		"check": "astro check && pnpm prettier:check",
		"dev": "astro dev",
		"prepare": "husky install",
		"preview": "astro preview",
		"start": "astro dev",
		"lint:scss": "stylelint \"src/**/*.{astro,scss,svelte}\"",
		"prettier:check": "prettier --cache --check --plugin-search-dir=. .",
		"format": "prettier --cache --write --plugin-search-dir=. ."
	},
	"lint-staged": {
		"*.{js,ts}": "eslint --fix",
		"*.{css,scss}": "stylelint --fix",
		"*.{html,json}": "prettier --write"
	},
	"stackblitz": {
		"startCommand": "pnpm start"
	},
	"dependencies": {
		"astro": "^1.9.0"
	},
	"devDependencies": {
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.48.0",
		"@typescript-eslint/parser": "^5.48.0",
		"astro-analytics": "^2.5.1",
		"astro-emoji": "^1.2.0",
		"astro-icon": "^0.8.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"open-props": "^1.5.3",
		"open-props-scss": "^0.4.0",
		"postcss": "^8.4.20",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.1",
		"sass": "^1.57.1",
		"stylelint": "^14.16.1",
		"stylelint-config-prettier": "^9.0.4",
		"stylelint-config-recommended-scss": "^8.0.0",
		"stylelint-config-standard": "^29.0.0",
		"stylelint-prettier": "^2.0.0",
		"typescript": "^4.9.4"
	}
}
